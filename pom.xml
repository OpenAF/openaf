  <modelVersion>4.0.0</modelVersion>
 
  <groupId>io.openaf</groupId>
  <artifactId>openaf</artifactId>
  <version>20250315</version>
  <packaging>pom</packaging>
  <name>io.openaf:openaf</name>
  <description>A Java and Javascript based "swiss-army knife" scripting DevOps tool for different automation 
    challenges with minimal requirements and footprint, extensible through packages (opacks).</description>
  <url>https://openaf.io</url>

  <licenses>
    <license>
      <name>Apache License, Version 2.0</name>
      <url>https://www.apache.org/licenses/LICENSE-2.0.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>

  <developers>
    <developer>
      <name>OpenAF Team</name>
      <email>openaf@openaf.io</email>
      <roles>
        <role>developer</role>
        <role>owner</role>
      </roles>
      <organization>OpenAF</organization>
      <organizationUrl>https://openaf.io</organizationUrl>
      <timezone>UTC</timezone>
    </developer>
  </developers>
  
  <scm>
    <connection>scm:git:git://github.com/openaf/openaf.git</connection>
    <developerConnection>scm:git:ssh://github.com:openaf/openaf.git</developerConnection>
    <url>http://github.com/openaf/openaf/tree/master</url>
  </scm>

  <build>
    <defaultGoal>exec:exec</defaultGoal>
    <directory>${project.basedir}/</directory>
    <outputDirectory>${project.build.directory}/bin</outputDirectory>
    <finalName>${project.artifactId}-${project.version}</finalName>
    <sourceDirectory>${project.basedir}/src</sourceDirectory>

    <!--pluginManagement-->
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.14.0</version>
            </plugin>
            <plugin>
              <groupId>org.codehaus.mojo</groupId>
              <artifactId>exec-maven-plugin</artifactId>
              <version>3.5.1</version>
              <executions>
                <execution>
                  <id>ojob-build</id>
                  <phase>compile</phase>
                  <goals>
                    <goal>exec</goal>
                  </goals>
                </execution>
                <execution>
                  <id>oaf-test</id>
                  <phase>test</phase>
                  <goals>
                    <goal>exec</goal>
                  </goals>
                  <configuration>
                    <executable>sh</executable>
                    <workingDirectory>.</workingDirectory>
                    <arguments>
                      <argument>oafTest.sh</argument>
                    </arguments>
                  </configuration>
                </execution>
              </executions>
              <configuration>
                <executable>ojob</executable>
                <workingDirectory>.</workingDirectory>
                <arguments>
                  <argument>build.yaml</argument>
                </arguments>
              </configuration>
            </plugin>   
            <plugin>
              <artifactId>maven-clean-plugin</artifactId>
              <version>3.5.0</version>
              <configuration>
                <skip>true</skip>
              </configuration>
            </plugin>         
        </plugins>
    <!--/pluginManagement-->
  </build>

